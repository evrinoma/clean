{
    "description": "clean project",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4 || 8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.10",
        "evrinoma/dashboard-bundle": "dev-master",
        "evrinoma/menu-bundle": "dev-master",
        "evrinoma/security-bundle": "dev-master",
        "evrinoma/user-bundle": "^2.1",
        "evrinoma/utils-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "friendsofsymfony/rest-bundle": "^3.0",
        "nelmio/api-doc-bundle": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/mime": "5.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.3.*",
        "symfony/process": "5.3.*",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/string": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "^5.3",
        "symfony/validator": "5.3.*",
        "symfony/web-link": "5.3.*",
        "symfony/yaml": "5.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.6",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/debug-bundle": "^5.3",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/var-dumper": "^5.3",
        "symfony/web-profiler-bundle": "^5.3",
        "evrinoma/test-utils-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "repositories": {
        "dashboard": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/DashBoardBundle.git"
        },
        "dto": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/DtoBundle.git"
        },
        "menu": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/MenuBundle.git"
        },
        "user": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/FOSUserBundle.git"
        },
        "utils": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/UtilsBundle.git"
        },
        "security-bundle": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/SecurityBundle.git"
        },
        "shell": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/ShellBundle.git"
        },
        "test-utils": {
            "type": "vcs",
            "url": "https://github.com/evrinoma/TestUtilsBundle.git"
        }
    }
}
